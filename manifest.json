{
    "manifest_version": 2,
    "name": "Daily Calendar Extension",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "dc.png",
        "default_popup": "popup.html",
        "default_title": "Daily Calendar"
    },
    "permissions": [
        "storage"
    ]

}